  <script>
$('body').terminal({
    hello: function(what) {
        this.echo('Hello, ' + what +
                  '. Wellcome to this terminal.');
    },
    marco: function() {
        this.echo('Polo!');
    },
    normal:function() {
        this.echo(normal.innerHTML);
        setTimeout(function() {
          window.location.replace("https://scaletty.com/aaronscaletty/ui");
        }, 1000)
    },
    help: function() {
        this.echo(help.innerHTML);
    },
    get: function(input) {
      if(input== 'cv'){

      }
      },


}, {
    greetings: greetings.innerHTML
